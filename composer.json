{
    "name": "mortezaa97/bookmarks",
    "description": "A Laravel package for managing user bookmarks, allowing users to save, view, and delete bookmarks",
    "keywords": [
        "mortezaa97",
        "bookmarks"
    ],
    "homepage": "https://github.com/mortezaa97/bookmarks",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Morteza jafari",
            "email": "mortezajafari76@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Mortezaa97\\Bookmarks\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mortezaa97\\Bookmarks\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mortezaa97\\Bookmarks\\BookmarksServiceProvider"
            ],
            "aliases": {
                "Bookmarks": "Mortezaa97\\Bookmarks\\BookmarksFacade"
            }
        }
    }
}
